{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ],
  "env": {
    "LOTARA_MODEL": "gemini-2.5-flash",
    "OPIK_PROJECT": "Lotara",
    "PYTHONPATH": "services/backend:src",
    "MAX_CONCURRENT_REQUESTS": "2"
  },
  "regions": ["iad1"],
  "functions": {
    "api/index.py": {
      "memory": 3008,
      "maxDuration": 300
    }
  },
  "crons": []
}
